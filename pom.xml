	<artifactId>cora-apptokenverifier-war</artifactId>
	<version>2.18-SNAPSHOT</version>
	<packaging>war</packaging>

	<name>apptokenverifier-war</name>
	<build>
		<finalName>apptokenverifier</finalName>
	</build>

	<properties>
		<initialize.version>1.4.0</initialize.version>
		<apptokenverifier.version>4.5.0</apptokenverifier.version>
		<!-- runtime -->
		<gatekeeper.version>4.4.0</gatekeeper.version>
		<gatekeepertokenprovider.version>2.12.0</gatekeepertokenprovider.version>
		<coralog4j.version>2.14.0</coralog4j.version>
		<spider.version>8.0.0</spider.version>
		<storage.version>8.1.0</storage.version>
